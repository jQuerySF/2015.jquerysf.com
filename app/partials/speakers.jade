mixin member(src, name, href, twitter)
  .team-member.column
    a.avatar-twitter(href=href)
      img.avatar(src=src, alt=name)
    p.avatar-name= name
    if twitter
      a.avatar-twitter(href=href)= twitter

section#speakers.team
  h2.headline Speakers
  h3 These amazing people, all in one place.

  .carousel-container
      span
      span

      //auto-generated from build script
      include ./speaker-carousel.jade

  a(href="/speakers.html") View Full Speaker List